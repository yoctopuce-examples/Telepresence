{
  "name": "demo_electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start_as_root": "electron . --no-sandbox",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/logo_black_1024.png - --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=icons/logo_black.ico --out=release-builds ",
    "package-lin-x64": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=icons/logo_black_1024.png --out=release-builds",
    "package-lin-ia32": "electron-packager . --overwrite --platform=linux --arch=ia32 --icon=icons/logo_black_1024.png --out=release-builds",
    "package-lin-arm": "electron-packager . --overwrite --platform=linux --arch=armv7l --icon=icons/logo_black_1024.png --out=release-builds",
    "package-lin-arm64": "electron-packager . --overwrite --platform=linux --arch=arm64 --icon=icons/logo_black_1024.png --out=release-builds"
  },
  "dependencies": {
    "yoctolib-es2017": "^1.10.38168"
  },
  "devDependencies": {
    "electron": "^7.1.3",
    "electron-packager": "^14.1.1"
  }
}
